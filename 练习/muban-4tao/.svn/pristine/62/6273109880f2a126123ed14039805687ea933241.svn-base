.m-form{
    margin-top: 20px;
    border: 1px solid #dbdbdb;
}
.m-form .group{
    overflow: hidden;
    border-bottom: 1px solid #dbdbdb;
}
.m-form .group span{
    float: left;
    width: 70px;
    height: 38px;
    line-height: 37px;
    text-align: right;
    padding-right: 10px;
    color: #000000;
    font-size: 14px;

}

.m-form .group p{
    overflow: hidden;
    padding: 3px;
    border-left: 1px solid #dbdbdb;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.m-form .group p > input{
    float: left;
    width: 352px;
    height: 30px;
    padding: 0 10px;
    outline: 0;
    border: 1px solid #cccccc;
}
.m-form .group p > textarea{
    float: left;
    width: 100%;
    height: 100px;
    padding: 5px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    border: 1px solid #cccccc;
    outline: 0;
    resize: none;
}
.m-form .group1 p >input{
    width: 260px;
}
.m-form .group1 em{
    display: inline-block;
    margin-left: 5px;
    text-align: center;
    width: 100px;
    height: 32px;
    line-height: 32px;
    background-color: #eeeeee;
}
.m-form .group .submit{
    width: 140px;
    height: 39px;
    text-align: center;
    line-height: 39px;
    color: #ffff;
    background-color: #000;
    text-align: center;
    border:none;
}

.m-form .group:last-child   {
    border-bottom: none;
}

