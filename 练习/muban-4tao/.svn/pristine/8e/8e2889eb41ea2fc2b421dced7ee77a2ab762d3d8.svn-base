.m-local{
    float: left;
    width: 390px;
    margin-right: 20px;
}
.m-local .local{
    font-size: 14px;
    line-height: 40px;
    color: #000000;
}
.m-local .local h3{
    font-size: 18px;
    line-height: 30px;
    margin-bottom: 7px;
}
.m-local .local p i{
    display: inline-block;
    vertical-align: middle;
    width: 14px;
    height: 14px;
    margin-right: 8px;
    background-position: center center;
    background-repeat: no-repeat;
}

.m-local .local1{
    border-top: 1px dashed #cccccc;
    margin-top: 13px;
    padding-top: 12px;
}
.m-local .local .pic{
    width: 80px;
    height: 80px;
    padding: 3px;
    border: 1px solid #dbdbdb;
    margin: 15px 25px;
    background-color: #fff;
}
.m-local .local .pic img{
    display: block;
    max-width: 100%;
    max-height: 100%;
}


/* 地图 */
.m-map{
    overflow: hidden;
    height: 380px;
    border: 1px solid #d0d0d0;
}
#allmap {
    width: 100%;
    height: 100%;
    overflow: hidden;
    margin: 0;
    font-family: "微软雅黑";
}

#allmap img {
    max-width: none;
    -webkit-transition: none;
    -o-transition: none;
    transition: none;
}
 #allmap .anchorBL img,
 #allmap .anchorBL span{
    display: none!important;
 }

