@media only screen and (max-width: 1199px) {
    .wp{
        width: 1000px;
    }
}
@media only screen and (max-width: 959px) {
    .wp{
        width: 100%;
        padding: 0 10px;
        -webkit-box-sizing: border-box;
        -moz-box-sizing: border-box;
        box-sizing: border-box;
    }
    .nav li .v1{
        padding: 0 15px;
    }
    .row-a1,
    .row-a2,
    .row-a3,
    .row-a4{
        padding: 30px 0;
    }
    .g-tit h3{
        font-size: 24px;
    }
    .ul-list1 li{
        width: 50%;
    }
    .ul-list2 li{
        float: none;
        width: auto;
    }
    .m-about .pic{
        float: none;
        margin: 0 auto 30px;
    }
    .m-about .txt h3{
        text-align: center;
    }
    .m-about .txt .more{
        margin-left: auto;
        margin-right: auto;
    }
    .fd-link{
        float: none;
        width: auto;
    }
    .fd-link dl:last-child{
        margin-right: 0;
    }
    .fd-tel{
        float: none;
        width: auto;
        text-align: center;
    }
}
@media only screen and (max-width: 767px) {
    .header{
        height: 50px;
    }
    .header .logo{
        line-height: 50px;
    }
    .header .logo img{
        height: 25px;
    }
    .nav{
        display: none;
    }
    .menuBtn{
        display: block;
    }
    .g-tit h3{
        font-size: 18px;
    }
    .ul-imgtxt li{
        width: 50%;
    }
    .ul-imgtxt .con .txt h5{
        font-size: 16px;
    }
    .ul-imgtxt .con .txt p{
        height: 110px;
        overflow: hidden;
        -ms-text-overflow: ellipsis;
        text-overflow: ellipsis;
    }
}
