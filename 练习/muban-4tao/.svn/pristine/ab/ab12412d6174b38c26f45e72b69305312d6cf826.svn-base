body {
    font-family: "微软雅黑", Arial;
    font-size: 12px;
    font-weight: normal;
    line-height: 1.7;
    color: #797979;
    background: #fff;
}
body.open{
    padding-top: 60px;
}
/*************header******************/
.wp {
    width: 1200px;
    margin: 0 auto;
}

.header {
    height: 60px;
    background-color: #fff;
    position: fixed;
    top: 0;
    right: 0;
    width: 100%;
    z-index: 1000;
}

.header .logo {
    float: left;
    line-height: 60px;
}

.header .logo img {
    display: inline-block;
    vertical-align: middle;
    height: 40px;
}
.header.fixed,
.header .logo.fixed{
    border-bottom: 1px solid #e9e9e9;
}
.menuBtn {
    display: none;
    position: fixed;
    z-index: 1000;
    top: 10px;
    right: 18px;
    /* margin-top: -15px; */
    width: 25px;
    height: 30px;
    cursor: pointer;
    transition: .3s;
    -ms-transition: .3s;
    -moz-transition: .3s;
    -webkit-transition: .3s;
    -o-transition: .3s;
}

.menuBtn b {
    display: block;
    height: 3px;
    background-color: #323232;
    margin: 4px 0;
    border-radius: 2px;
    transition: .3s;
    -ms-transition: .3s;
    -moz-transition: .3s;
    -webkit-transition: .3s;
    -o-transition: .3s;
}
.menuBtn.open b{
    display: none;
    margin: 0;
    position: absolute;
    left: 0px;
    top: 50%;
    margin-top: -5px;
    width: 30px;
}   
.menuBtn.open b:first-child{
 display: block;
    transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
}
.menuBtn.open b:last-child {
    display: block;
    transform: rotate(-45deg);
    -webkit-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    }
.nav {
    float: right;
}

.nav>li {
    float: left;
    position: relative;
}

.nav li .v1 {
    display: block;
    padding: 0 32px;
    line-height: 60px;
    color: #2a333c;
    font-size: 14px;
    font-weight: normal;
    text-align: center;
    -webkit-transition: 0.5s;
    -o-transition: 0.5s;
    transition: 0.5s;
}

.nav li .v1 span {
    display: inline-block;
    width: 0;
    height: 0;
    margin-left: 2px;
    vertical-align: middle;
    border-top: 4px solid;
    border-right: 4px solid transparent;
    border-left: 4px solid transparent;
}
.nav li .v1:hover{
    color: #4e97d9;
}
.nav li .sub {
    position: absolute;
    top: 120%;
    left: 0;
    min-width: 160px;
    z-index: 4;
    background: #fff;
    -webkit-box-shadow: 0px 4px 5px 0px rgba(0, 0, 0, 0.15);
    -moz-box-shadow: 0px 4px 5px 0px rgba(0, 0, 0, 0.15);
    box-shadow: 0px 4px 5px 0px rgba(0, 0, 0, 0.15);
    border: 1px solid rgba(0, 0, 0, .15);
    transition: .5s;
    opacity: 0;
    height: 0;
    visibility: hidden;
}

.nav li .sub a {
    color: #2a333c;
    display: block;
    width: 100%;
    padding: 6px 20px 6px 20px;
    text-align: left;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.nav li .sub a:hover{
    color: #4e97d9;
    background-color: #f5f5f5;
}

.nav li .sub.ani{
    visibility: visible;
    height: auto;
    top: 100%;
    opacity: 1;
}
/* banner箭头  小圆点*/
.banner.slick-slider .slick-arrow {
    font-size: 0;
    text-indent: 9999px;
    border: none;
    z-index: 5;
    cursor: pointer;
    outline: 0;
    width: 40px;
    height: 40px;
    position: absolute;
    top: 50%;
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);
    background-color: transparent;
}

.banner.slick-slider .slick-prev {
    left: 25px;
    background: url("../images/ban-arrow.png") no-repeat -190px 50%;
    opacity: 0.75;
}

.banner.slick-slider .slick-next {
    right: 25px;
    background: url("../images/ban-arrow.png") no-repeat -66px 50%;
    opacity: 0.75;

}

.banner.slick-slider .slick-prev:hover {
    background: url("../images/ban-arrow.png") no-repeat -130px 50%;
    opacity: 1;
}

.banner.slick-slider .slick-next:hover {
    background: url("../images/ban-arrow.png") no-repeat -6px 50%;
    opacity: 1;
}

.banner.slick-slider .slick-dots {
    position: absolute;
    left: 50%;
    -webkit-transform: translateX(-50%);
    -ms-transform: translateX(-50%);
    -o-transform: translateX(-50%);
    transform: translateX(-50%);
    bottom: 20px;
    width: 100%;
    text-align: center;
}

.banner.slick-slider .slick-dots li {
    display: inline-block;
    width: 9px;
    height: 9px;
    border-radius: 50%;
    margin: 0 2px;
    opacity: 0.5;
    background-color: #2a333c;
    cursor: pointer;
}

.banner.slick-slider .slick-dots button {
    display: none;
}

.banner.slick-slider .slick-dots li.slick-active {
    background-color: #4e97d9;
}




/* 页脚 */
.footer {
    background-color: #fff;
}

.fd-top {
    overflow: hidden;
    padding: 40px 0 20px;
}

.fd-link {
    overflow: hidden;
    float: left;
}

.fd-link dl {
    float: left;
    margin-right: 100px;
}

.fd-link dt a {
    color: #2a333c;
    font-size: 18px;
    font-weight: normal;
    padding-left: 20px;
    margin-bottom: 10px;
    display: block;
}

.fd-link dd a {
    display: block;
    color: #5e7387;
    font-size: 14px;
    padding-left: 20px;
    margin-bottom: 5px;
}

.fd-link dd a:hover {
    color: #62a8ea;
    text-decoration: underline;
}

.fd-tel {
    float: left;
    color: #5e7387;
    font-size: 26px;
    margin-top: 15px;
}

.fd-tel span {
    display: block;
    color: #aaa;
    font-size: 12px;
    line-height: 1.5;
    /* padding-top: 8px; */
}

.copy {
    line-height: 65px;
    font-size: 14px;
    border-top: 1px solid #f0f2f5;
    color: #aaaaaa;
    text-align: center;
}

.backTop {
    display: none;
    width: 35px;
    height: 35px;
    position: fixed;
    right: 5px;
    bottom: 60px;
    cursor: pointer;
    z-index: 7;
    background: #62a8ea url(../images/top.png) no-repeat center center;
}
.backTop:hover {
    background-color: #89BCEB;
}